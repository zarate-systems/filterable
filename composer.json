{
    "name": "zaratesystems/laravel-filterable",
    "description": "Apply filters to your Eloquent models in Laravel",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "zaratedev",
            "email": "zaratedev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "orchestra/testbench": "^4.9|^5.8|^6.3",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Zarate\\Filterable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zarate\\Filterable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zarate\\Filterable\\FilterableServiceProvider"
            ]
        }
    }
}
